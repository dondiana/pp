        "\n",
        "import numpy as np\n",
        "A = np.array([[1, 2], [3, 4]])\n",
        "B = np.array([[5, 6], [7, 8]])\n",
        "res1 = np.dot(A, B)  # or A @ B\n",
        "print(\"1. Matrix Multiplication:\\n\", res1)\n",
        "\n",
        "res2 = A.T\n",
        "print(\"2. Matrix Transpose:\\n\",res2)\n",
        "\n",
        "res3 = np.linalg.inv(A)\n",
        "print(\"3. Inverse of Matrix:\\n\", res3)\n",
        "\n",
        "res4 = np.linalg.det(A)\n",
        "print(\"4. Determinant of Matrix:\", res4)\n",
        "\n",
        "eigenvalues, eigenvectors = np.linalg.eig(A)\n",
        "print(\"5. Eigenvalues:\\n\", eigenvalues)\n",
        "print(\"6. Eigenvectors:\\n\", eigenvectors)\n",
        "\n",
        "C = np.array([[3, 1], [1, 2]])\n",
        "D = np.array([9, 8])\n",
        "solution = np.linalg.solve(C, D)\n",
        "print(\"7. Solution to the system of linear equations:\", solution)\n",
        "\n",
        "x = np.array([0, 1, 2, 3, 4])\n",
        "y = np.array([0, 1, 4, 9, 16])\n",
        "gradient = np.gradient(y, x)\n",
        "print(\"8. Numerical Differentiation (Gradient):\", gradient)\n",
        "\n",
        "m = np.linspace(0, 10, 100)\n",
        "n = np.sin(m)\n",
        "integral = np.trapz(n, m)\n",
        "print(\"9. Numerical Integration (Trapezoidal Rule):\", integral)\n",
        "\n",
        "p = np.poly1d([3, 2, 1])  # 3x^2 + 2x + 1\n",
        "derivative_p = np.polyder(p)\n",
        "print(\"10. Polynomial Differentiation:\", derivative_p)\n",
        "\n",
        "roots = np.roots(p)\n",
        "print(\"11. Polynomial Roots:\", roots)\n",
        "\n",
        "data = np.array([1, 2, 3, 4, 5])\n",
        "mean_value = np.mean(data)\n",
        "print(\"12. Mean:\", mean_value)\n",
        "\n",
        "variance_value = np.var(data)\n",
        "print(\"13. Variance:\", variance_value)\n",
        "\n",
        "std_deviation = np.std(data)\n",
        "print(\"14. Standard Deviation:\", std_deviation)\n",
        "\n",
        "k = np.array([1, 2, 3, 4, 5])\n",
        "l = np.array([5, 6, 7, 8, 9])\n",
        "correlation = np.corrcoef(k, l)\n",
        "print(\"15. Correlation Coefficient:\\n\", correlation)\n",
        "\n"
